{
    "folders": {
        "business_rules": {
            "table": "sys_script",
            "key": "name",
            "fields": {
                "js": "script"
            },
            "subDirPattern": "collection/when"
        },
        "client_scripts": {
            "table": "sys_script_client",
            "key": "name",
            "fields": {
                "js": "script"
            },
            "subDirPattern": "table/type"
        },
        "email_scripts": {
            "table": "sys_script_email",
            "key": "name",
            "fields": {
                "js": "script"
            }
        },
        "inbound_actions": {
            "table": "sysevent_in_email_action",
            "key": "name",
            "fields": {
                "js": "script"
            }
        },
        "processors": {
            "table": "sys_processor",
            "key": "name",
            "fields": {
                "js": "script"
            }
        },
        "script_actions": {
            "table": "sysevent_script_action",
            "key": "name",
            "fields": {
                "js": "script"
            },
            "subDirPattern": "event_name"
        },
        "script_includes": {
            "table": "sys_script_include",
            "key": "name",
            "fields": {
                "js": "script"
            },
            "subDirPattern": "scope_<sys_scope>/client_<client_callable>"
        },
        "style_sheets": {
            "table": "content_css",
            "key": "name",
            "fields": {
                "css": "style"
            }
        },
        "ui_actions": {
            "table": "sys_ui_action",
            "key": "name",
            "fields": {
                "js": "script"
            },
            "subDirPattern": "table/client_<client>"
        },
        "ui_macros": {
            "table": "sys_ui_macro",
            "key": "name",
            "fields": {
                "xhtml": "xml"
            },
            "subDirPattern": "category"
        },
        "ui_pages": {
            "table": "sys_ui_page",
            "key": "name",
            "fields": {
                "xhtml": "html",
                "client.js": "client_script",
                "server.js": "processing_script"
            },
            "subDirPattern": "category"
        },
        "ui_scripts": {
            "table": "sys_ui_script",
            "key": "name",
            "fields": {
                "js": "script"
            }
        },
        "dynamic_content_blocks": {
            "table": "content_block_programmatic",
            "key": "name",
            "fields": {
                "xhtml": "programmatic_content"
            },
            "subDirPattern": "category"
        },
        "email_templates": {
            "table": "sysevent_email_template",
            "key": "name",
            "fields": {
                "html": "message_html"
            },
            "subDirPattern": "collection"
        },
        "fix_scripts": {
            "table": "sys_script_fix",
            "key": "name",
            "fields": {
                "js": "script"
            }
        },
        "catalog_client_scripts": {
            "table": "catalog_script_client",
            "key": "name",
            "fields": {
                "js": "script"
            },
            "subDirPattern": "applies_to_<applies_to>/type"
        },
        "sys_web_service": {
            "table": "sys_web_service",
            "key": "name",
            "fields": {
                "js": "script"
            }
        },
        "web_service_resources": {
            "table": "sys_ws_operation",
            "key": "name",
            "fields": {
                "js": "operation_script"
            },
            "subDirPattern": "web_service_definition"
        },
        "sp_widgets": {
            "table": "sp_widget",
            "key": "name",
            "fields": {
                "client.js": "client_script",
                "demo.js": "demo_data",
                "server.js": "script",
                "html": "template",
                "css": "css"
            },
            "subDirPattern": "servicenow_<servicenow>"
        },
        "sp_header_footer": {
            "table": "sp_header_footer",
            "key": "name",
            "fields": {
                "client.js": "client_script",
                "demo.js": "demo_data",
                "server.js": "script",
                "html": "template",
                "css": "css"
            },
            "subDirPattern": "servicenow_<servicenow>"
        },
        "sp_providers": {
            "table": "sp_angular_provider",
            "key": "name",
            "fields": {
                "js": "script"
            },
            "subDirPattern": "type"
        },
        "sp_css": {
            "table": "sp_css",
            "key": "name",
            "fields": {
                "css": "css"
            }
        },
        "sp_ng_templates": {
          "table": "sp_ng_template",
          "key": "id",
            "fields": {
                "html": "template"
            },
            "subDirPattern": "sp_widget"
        },








        "sys_properties": {
            "table": "sys_properties",
            "key": "name",
            "subDirPattern": "<sys_scope>",
            "_custom": true
        },
        "sys_app_module": {
            "_info": "Nav modules",
            "table": "sys_app_module",
            "key": "title",
            "subDirPattern": "application",
            "searchConstraint": "titleISNOTEMPTY",
            "_info_searchConstraint": "Avoid separators",
            "_custom": true
        },
        "sys_data_policy2": {
            "table": "sys_data_policy2",
            "key": "model_table",
            "subDirPattern": "sys_package",
            "_custom": true
        },
        "sysevent_register": {
            "table": "sysevent_register",
            "key": "event_name",
            "subDirPattern": "sys_scope/table",
            "_custom": true
        },
        "sys_choice": {
            "_info": "Choices",
            "table": "sys_choice",
            "key": "element",
            "subDirPattern": "name",
            "noClassName": true,
            "_custom": true
        },
        "tables": {
            "_info": "Tables",
            "table": "sys_db_object",
            "key": "name",
            "subDirPattern": "sys_package",
            "searchConstraint": "sys_nameISNOTEMPTY",
            "_custom": true
        },
        "sys_dictionary": {
            "_info": "fields",
            "table": "sys_dictionary",
            "key": "element",
            "subDirPattern": "sys_scope/name",
            "_note": "name = table system name. sys_ fields have no scope and we do not want them.",
            "searchConstraint": "elementISNOTEMPTY^sys_scopeISNOTEMPTY",
            "_custom": true
        },
        "sys_documentation": {
            "_info": "Field labels",
            "table": "sys_documentation",
            "key": "element",
            "subDirPattern": "sys_scope/name",
            "searchConstraint": "language=en^sys_scopeISNOTEMPTY",
            "_custom": true
        },
        "sys_user_role": {
            "_info": "user roles",
            "table": "sys_user_role",
            "key": "name",
            "subDirPattern": "sys_package",
            "_custom": true
        },
        "sys_security_acl": {
            "_info": "ACLs",
            "table": "sys_security_acl",
            "key": "sys_name",
            "fields": {
                "js": "script"
            },
            "subDirPattern": "type/sys_name/operation",
            "_custom": true
        }
    }
}
